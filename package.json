{
  "name": "super-cache",
  "version": "1.0.0",
  "description": "All around cache solution for nodejs and browser. Supports most of cache drivers, multi-layered cache, namespacing and value injecting.",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "scripts": {
    "build": "run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "run-s build test:*",
    "test:lint": "eslint src --ext .ts",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test:unit": "jest",
    "test:unit:coverage": "jest --coverage",
    "watch:build": "tsc -p tsconfig.json -w",
    "watch:test": "jest --watch"
  },
  "author": "Evgeni Dikerman",
  "license": "MIT",
  "files": [
    "dist",
    "!**/*.spec.*",
    "!**/*.json",
    "README.md"
  ],
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.12",
    "@types/object-hash": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^5.0.7",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "object-hash": "^3.0.0",
    "unstorage": "https://github.com/ur92/unstorage/archive/refs/tags/1.6.0.tar.gz"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 4
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
